{
  "name": "@accera/solar-components",
  "version": "0.0.59-beta",
  "description": "Web Components implementation of Solar Design System",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --drocs",
    "publish-latest": "npm run build && npm publish --tag latest",
    "publish-beta": "npm run build && npm publish --tag beta",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "storybook": "start-storybook -p 6006 -s .",
    "build-storybook": "build-storybook",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "lint": "npm run lint.ts && npm run lint.sass",
    "lint.fix": "npm run lint.ts.fix && npm run lint.sass.fix",
    "lint.sass": "stylelint src/**/*.scss",
    "lint.sass.fix": "npm run lint.sass -- --fix",
    "lint.ts": "tslint --project .",
    "lint.ts.fix": "tslint --project . --fix"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "popper.js": "^1.15.0",
    "ramda": "^0.26.1",
    "vanilla-masker": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@stencil/core": "0.17.3-0",
    "@stencil/sass": "^0.1.1",
    "@storybook/addon-knobs": "^5.0.5",
    "@storybook/addon-notes": "^5.0.5",
    "@storybook/html": "^5.0.10",
    "@types/jest": "^24.0.11",
    "conventional-changelog": "^3.1.3",
    "husky": "^2.1.0",
    "jest": "^24.7.1",
    "stylelint": "^10.0.1",
    "stylelint-order": "^3.0.0",
    "tslint": "^5.16.0",
    "tslint-ionic-rules": "0.0.21",
    "tslint-react": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/stencil-component-starter.git"
  },
  "author": "Accera Tech",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/accera-tech/solar-components/issues"
  },
  "homepage": "https://github.com/accera-tech/solar-components"
}
