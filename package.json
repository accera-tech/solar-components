{
  "name": "@accera/solar-components",
  "version": "0.0.39.1-beta",
  "description": "Web Components implementation of Solar Design System",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "publish-latest": "npm run build && npm publish --tag latest",
    "publish-beta": "npm run build && npm publish --tag beta",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "storybook": "start-storybook -p 6006 -s .",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "ramda": "^0.26.1",
    "vanilla-masker": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@stencil/core": "~0.15.1",
    "@stencil/sass": "^0.1.1",
    "@storybook/addon-knobs": "^5.0.5",
    "@storybook/addon-notes": "^5.0.5",
    "@storybook/html": "^5.0.3",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "jest": "^23.6.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "rollup-plugin-strip-code": "^0.2.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/stencil-component-starter.git"
  },
  "author": "Accera Tech",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/accera-tech/solar-components/issues"
  },
  "homepage": "https://github.com/accera-tech/solar-components"
}
